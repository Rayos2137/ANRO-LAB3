<?xml version="1.0" ?>
<robot name="dobot_magician">
  <!-- Definicje materiałów (DODANE) -->
  <material name="grey">
    <color rgba="0.7 0.7 0.7 1.0"/>
  </material>
  <material name="blue">
    <color rgba="0.0 0.0 0.8 1.0"/>
  </material>
  <!-- Baza robota -->
  <link name="base_link">
    <visual>
      <geometry>
        <box size="0.2 0.2 0.05"/>
      </geometry>
      <material name="grey"/>
    </visual>
  </link>
  <!-- Złącze 1:Obrót bazy -->
  <joint name="base_to_link1" type="revolute">
    <parent link="base_link"/>
    <child link="link1"/>
    <axis xyz="0 0 1"/>
    <limit effort="100" lower="-1.5708" upper="1.5708" velocity="1.0"/>
    <origin rpy="0 0 0" xyz="0 0 0.069"/>
  </joint>
  <!-- baza obrotowa  -->
  <link name="link1">
    <visual>
      <geometry>
        <cylinder length="0.088" radius="0.07"/>
      </geometry>
      <material name="blue"/>
    </visual>
  </link>
  <!-- Złącze 2:Ramie -->
  <joint name="link1_to_link2" type="revolute">
    <parent link="link1"/>
    <child link="link2"/>
    <axis xyz="1 0 0"/>
    <limit effort="100" lower="-1.5708" upper="1.5708" velocity="1.0"/>
    <origin rpy="0.78 0 0" xyz="0 0 0.044"/>
  </joint>
  <!-- ramie 1  -->
  <link name="link2">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.0675"/>
      <geometry>
        <box size="0.03 0.03 0.135"/>
      </geometry>
      <material name="grey"/>
    </visual>
  </link>
  <!-- Złącze 3:Przedramie -->
  <joint name="link2_to_link3" type="revolute">
    <parent link="link2"/>
    <child link="link3"/>
    <axis xyz="1 0 0"/>
    <limit effort="100" lower="-1.5708" upper="1.5708" velocity="1.0"/>
    <origin rpy="0.78 0 0" xyz="0 0 0.135"/>
  </joint>
  <!-- przedramie  -->
  <link name="link3">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.0735"/>
      <geometry>
        <box size="0.03 0.03 0.147"/>
      </geometry>
      <material name="grey"/>
    </visual>
  </link>
  <!-- Złącze 4:uchwyt narzedzia -->
  <joint name="link3_to_link4" type="revolute">
    <parent link="link3"/>
    <child link="link4"/>
    <axis xyz="1 0 0"/>
    <limit effort="100" lower="-1.5708" upper="1.5708" velocity="1.0"/>
    <origin rpy="0.78 0 0" xyz="0 0 0.147"/>
  </joint>
  <!-- nadgarstek  -->
  <link name="link4">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.03"/>
      <geometry>
        <box size="0.01 0.01 0.06"/>
      </geometry>
      <material name="grey"/>
    </visual>
  </link>
  <!-- Złącze 5:narzedzie -->
  <joint name="link4_to_link5" type="revolute">
    <parent link="link4"/>
    <child link="link5"/>
    <axis xyz="0 0 1"/>
    <limit effort="100" lower="-1.5708" upper="1.5708" velocity="1.0"/>
    <origin rpy="0 0 0" xyz="0 0 0.06"/>
  </joint>
  <!-- dlon  -->
  <link name="link5">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.005"/>
      <geometry>
        <box size="0.07 0.05 0.01"/>
      </geometry>
      <material name="grey"/>
    </visual>
  </link>
</robot>
